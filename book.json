{
    "title": "Linkkey Documentation",
    "description": "Reusable functions and components for the SNS apps ",
    "plugins": [
        "-sharing",
        "livereload",
        "hide-element",
        "search-pro",
        "expandable-chapters",
        "chapter-fold",
        "sectionx",
        "splitter",
        "lightbox",
        "copy-code-button",
        "anchor-navigation-ex",
        "favicon",
        "insert-logo",
        "sharing-plus",
        "github",
        "my-toolbar"
    ],
    "styles": {
        "website": "./assets/css/websiteStyle.css"
    },
    "pluginsConfig": {
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "anchor-navigation-ex": {
            "multipleH3": false,
            "showLevel": false
        },
        "favicon": {
            "shortcut": "assets/img/favicon.ico"
        },
        "insert-logo": {
            "url": "https://i.ibb.co/zN4mpnG/linkkey-logo.png",
            "style": "max-height: 60px; min-height: 60px; border-radius:16px;box-shadow: 0 6px 12px rgb(0 0 0 / 18%);"
        },
        "github": {
            "url": "https://github.com/Link-Key"
        },
        "sharing-plus": {
            "twitter": true
        },
        "my-toolbar": {
            "buttons": [
                {
                    "label":"sns-app",
                    "text": "SNS-APP",
                    "icon": "fa",
                    "url": "https://sns.chat/"
                },
                {
                    "label":"linkkey",
                    "text": "Linkkey",
                    "icon": "fa",
                    "url": "https://www.linkkey.io/"
                },
                {
                    "label":"twitter",
                    "text": " ",
                    "icon": "fa fa-twitter",
                    "url": "https://twitter.com/LinkkeyOfficial"
                },
                {
                    "label":"telegram",
                    "text": " ",
                    "icon": "fa fa-paper-plane",
                    "url": "https://t.me/linkkeydao"
                }
            ]
        }
    }
}